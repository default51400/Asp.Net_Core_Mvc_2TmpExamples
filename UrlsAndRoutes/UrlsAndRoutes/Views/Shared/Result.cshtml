@model Result

@{ Layout = null; }

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Routing</title>
    <link rel="stylesheet" asp-href-include="lib/bootstrap/dist/css/*.min.css" />
</head>
<body class="m-1 p-1">
    <table class="table table-bordered table-striped table-sm">
        <tr><th>Controller:</th><td>@Model.Controller</td></tr>
        <tr><th>Action:</th><td>@Model.Action</td></tr>
        @foreach (string key in Model.Data.Keys)
        {
            <tr><th>@key :</th><td>@Model.Data[key]</td></tr>
        }
    </table>
    @*1 <a asp-controller="Home" asp-action="Index" asp-route-id="Hello"
        asp-protocol="https" asp-host="myserver.domain.com"
        asp-fragment="myFragment">This is another controller</a>*@
    @*2 <a asp-controller="Home" asp-action="CustomVariable">This is URL</a>
        <a asp-route="out"> This is out URL</a>*@
    @*3 <p>URL: @Url.Action("CustomVariable", "Home", new { id = 100 })</p>*@

    <p>URL: @Url.Action(null, null, new { legacyurl ="/articles/Windows_3.1_Overview.html"})</p>

</body>
</html>